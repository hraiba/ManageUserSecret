{
  "name": "usersecret",
  "displayName": "Asrar.net",
  "description": "An extension to manage .NET projects user secrets file",
  "version": "0.6.2",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "MoHraiba",
  "icon": "images/asrar.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/hraiba/ManageUserSecret.git"
  },
  "keywords": [
    "user secret",
    "user secrets",
    "manage user secret",
    "manage user secrets",
    "dotnet user secret",
    "dotnet user secrets",
    ".NET user secrets",
    ".NET user secret",
    "csproj user secret",
    "csproj user secrets",
    ".NET",
    "C#",
    "CSharp",
    "ASP.NET"
  ],
  "license": "MIT",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "command": "dotnet.manageUserSecrets",
          "when": "resourceExtname == .csproj",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "dotnet.manageUserSecrets",
        "title": "Manage .NET User Secrets"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "sinon": "^19.0.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@types/xml2js": "^0.4.14",
    "fast-glob": "^3.3.2",
    "fg": "^0.0.1",
    "uuid": "^11.0.3",
    "xml2js": "^0.6.2"
  }
}
